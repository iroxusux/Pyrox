    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Pyrox",
            "type": "debugpy",
            "request": "launch",
            "module": "pyrox",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Debug File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}"
        }
    ]
}